    <div class="greeting">
      <a href="/users/<%= current_user.id %>">
      <p>こんにちは、<%= current_user.name %> さん</p>
      </a>
    </div>
  <% end %>
    <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
      <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
      <%= render partial: 'prototype', collection: @prototypes, as: :prototype %>
    </div>
  </div>
</main>